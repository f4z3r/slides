{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.14.2/.schema/devbox.schema.json",
  "packages": [
    "python312Packages.presenterm-export@0.2.7",
    "presenterm@0.15.1",
    "kitty@0.42.2",
  ],
  "shell": {
    "scripts": {
      "kitty": [
        "KITTY_DISABLE_WAYLAND=1 kitty",
      ],
      "present": [
        "presenterm -xpP slides.md -c ../assets/presenterm/config.yaml"
      ],
      "notes": [
        "presenterm -l slides.md"
      ]
    }
  }
}
